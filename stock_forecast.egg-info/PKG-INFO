from src.features.ta_factors import add_technical_indicators
from src.strategies.equal_weight import weights
from src.backtest.engine import run

# 1. ë°ì´í„° ìˆ˜ì§‘
prices = collect_data(['AAPL', 'MSFT'], '2020-01-01', '2024-12-31')

# 2. í”¼ì²˜ ìƒì„±
